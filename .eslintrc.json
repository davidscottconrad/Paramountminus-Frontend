{
    "env": {
      "browser": true,
      "es2021": true
    }, 
    "globals": {
      "process": "readonly"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "vue",
      "prettier"
    ],
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "prettier/prettier": ["error", { "endOfLine": "auto", "printWidth": 100 }]
    }
  }
  