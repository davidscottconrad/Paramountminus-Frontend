{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "process": "readonly"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "prettier"
  ],
  "rules": {
    "max-len": "off",
    "prettier/prettier": [
      "off",
      {
        "endOfLine": "auto",
        "printWidth": 100
      }
    ]
  }
}